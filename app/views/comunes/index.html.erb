<h1>Listing comunes</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comunes.each do |comune| %>
  <tr>
    <td><%= comune.name %></td>
    <td><%= link_to 'Show', comune %></td>
    <td><%= link_to 'Edit', edit_comune_path(comune) %></td>
    <td><%= link_to 'Destroy', comune, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

